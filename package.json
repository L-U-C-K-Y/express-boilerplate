{
  "name": "boilerplate_api",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose up boilerplate_api",
    "build": "tsc --skipLibCheck",
    "watch": "docker-compose up builder",
    "docker-build": "BRANCH_NAME=$(git rev-parse --abbrev-ref HEAD | sed 's/[/]/-/g'); docker-compose build builder && docker tag app:local app:${BRANCH_NAME}",
    "docker-build-prod": "docker build -t boilerplate-api:latest -f ./docker/prod/Dockerfile .",
    "down": "docker-compose down -v",
    "format": "docker-compose run --rm js ./node_modules/.bin/prettier --write \"**/**/*.ts\" \"!**/**/*.d.ts\"",
    "lint": "docker-compose run --rm js ./node_modules/.bin/eslint -c ./eslint.json --ext .ts ./",
    "lint-fix": "docker-compose run --rm js ./node_modules/.bin/eslint -c ./eslint.json --ext .ts --fix ./",
    "integration-tests": "cd build && node ../node_modules/.bin/mocha \"tests/**/*.spec.js\" --file=\"tests/bootstrap.js\" --exit",
    "services-units": "cd build && node ../node_modules/.bin/mocha \"src/**/*.spec.js\"",
    "integration": "docker-compose run --rm js npm run integration-tests",
    "units": "docker-compose run --rm js npm run services-units",
    "shell": "docker-compose run --rm js sh",
    "generate-migration": "docker-compose run --rm js ./node_modules/.bin/typeorm migration:generate -f ./build/config/db -n",
    "run-migrations": "docker-compose run --rm js ./node_modules/.bin/typeorm migration:run -f ./build/config/db",
    "revert-migration": "docker-compose run --rm js ./node_modules/.bin/typeorm migration:revert -f ./build/config/db",
    "create-migration": "docker-compose run --rm js ./node_modules/.bin/typeorm migration:create -f ./build/config/db -n",
    "plop": "plop",
    "generate-schema": "docker-compose run --rm js ./node_modules/.bin/graphql-codegen"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@tshio/awilix-resolver": "^1.0.2",
    "@tshio/command-bus": "^1.0.2",
    "@tshio/event-dispatcher": "^1.0.2",
    "@tshio/logger": "^1.0.4",
    "@tshio/query-bus": "^1.0.3",
    "@tshio/security-client": "0.0.19",
    "apollo-server-express": "^2.20.0",
    "awilix": "^4.3.1",
    "celebrate": "12.2.0",
    "class-transformer": "^0.3.2",
    "cors": "2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "graphql": "^15.5.0",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "pg": "^8.5.1",
    "redis": "^3.0.2",
    "source-map-support": "^0.5.19",
    "swagger-express-ts": "^1.0.1",
    "swagger-ui-dist": "^3.42.0",
    "ts-pipe-compose": "^0.2.1",
    "typeorm": "^0.2.31",
    "typeorm-naming-strategies": "^2.0.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "4.17.5",
    "@types/express-serve-static-core": "4.17.5",
    "@types/helmet": "4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.25",
    "@types/redis": "^2.8.28",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-unicorn": "^28.0.0",
    "husky": "^4.3.8",
    "lodash.kebabcase": "^4.1.1",
    "mocha": "^8.2.1",
    "nock": "^13.0.7",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "typescript": "^4.1.4"
  }
}
